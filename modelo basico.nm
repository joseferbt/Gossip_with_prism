mdp 

module x
	s:[1..3];
	f1:bool init false;
	f2:bool init false;
	f3:bool init false;
//secuencial
//	[]s=1->  (s'=2)&(f1'=true);
//	[]s=2->  (s'=3)&(f3'=true);
//	[]s=3->  (s'=1)&(f2'=true);
// nodeterminisitico
	[]true->  (s'=1)&(f1'=true);
	[]true->  (s'=3)&(f3'=true);
	[]true->  (s'=2)&(f2'=true);

	[round] f1&f2&f3-> (f1'=false)&(f2'=false)&(f3'=false);
endmodule
rewards "rounds"
	f1&f2&f3 : 1;
endrewards 

label "done"= f1&f2&f3;

